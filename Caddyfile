{
	persist_config off
	admin off
}

(common_config) {
	# Turn on the Web/file server
	file_server

	# The site root
	root * /srv

	#	Because we should
	encode zstd gzip

	# On OCP we should log to stdout so Prometheus can
	# slurp up the logs for human consumption.
	log {
		#errors stdout
		output stdout
		format transform "{common_log}"
		level info
	}

	handle_errors {
		rewrite * /error.html
		file_server
	}
}

